{
  "name": "remote-configs-prisma",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "export NODE_ENV=development && ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "export NODE_ENV=production && node build/server",
    "clean": "rm -rf build",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema"
  },
  "keywords": [],
  "author": "sy4rif",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.1",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@prisma/client": "^5.6.0",
    "argon2": "^0.31.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.22.4",
    "graphql-shield": "^7.6.5",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "nexus": "^1.3.0"
  }
}
